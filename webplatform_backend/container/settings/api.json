{
  "num_cores": 1,
  "host": "localhost",
  "container": {
    "name": "api",
    "volumes": {
      "service": {
        "rel": true,
        "path": "../app"
      }
    },
    "ports": {
      "8000": 8000
    },
    "mem_limit": "4g"
  },
  "port": 8080,
  "user-package": "services.ldap.auth",
  "user-package-default": "webplatform_auth.user"
}